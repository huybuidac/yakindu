  <behavior xsi:type="dml:OpaqueBehaviorSpecification" xmi:id="_i5UkMdR6Ed-6Zv_9XpuCAw" behavior="behavior {&#xA;&#xA;&#x9;check checkInput() {&#xA;&#x9;&#x9;var inputUnit = unit(?);&#xA;&#x9;&#x9;foreach (e in input) {&#xA;&#x9;&#x9;&#x9;if (e is real(?)) {&#xA;&#x9;&#x9;&#x9;&#x9;if (inputUnit == unit(?)) {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;inputUnit = e.unit;&#xA;&#x9;&#x9;&#x9;&#x9;} elseif (inputUnit != e.unit) {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;fatal &quot;Input values must have same unit&quot;;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;} else {&#xA;&#x9;&#x9;&#x9;&#x9;fatal &quot;Input values must be numeric&quot;;&#xA;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;}&#xA;&#x9;}&#xA;&#x9;&#xA;&#x9;pre checkInput();&#xA;&#x9;&#xA;&#x9;compute {&#xA;&#x9;&#x9;var sum;&#xA;&#x9;&#x9;var first = true;&#xA;&#x9;&#x9;foreach (e in input) {&#xA;&#x9;&#x9;&#x9;if (first) {&#xA;&#x9;&#x9;&#x9;&#x9;sum = e;&#xA;&#x9;&#x9;&#x9;&#x9;first = false;&#xA;&#x9;&#x9;&#x9;} else {&#xA;&#x9;&#x9;&#x9;&#x9;sum = sum + e;&#xA;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;}&#xA;&#x9;&#x9;return sum;&#xA;&#x9;}&#xA;&#xA;}&#xA;"/>
